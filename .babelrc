{
  "env": {
    "development": {
      "presets": "next/babel",
      "plugins": [
        // Convert css file to js for styled-jsx
        [
          "wrap-in-js",
          {
            "extensions": ["css$"]
          }
        ],
        // Enable .env
        "inline-dotenv",
        // Strip out flow typings
        "transform-flow-strip-types"
      ]
    },
    "production": {
      "presets": "next/babel",
      "plugins": [
        // Enable .env
        "transform-inline-environment-variables"
      ]
    },
    "test": {
      "presets": [
        ["env", { "modules": "commonjs" }],
        "next/babel"
      ]
    }
  }
}
